<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Patient.
    -->
    <changeSet id="20231107131558-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="medecin_id"
                                 baseTableName="patient"
                                 constraintName="fk_patient__medecin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="medecin"
                                 />

        <addForeignKeyConstraint baseColumnNames="etablissement_id"
                                 baseTableName="patient"
                                 constraintName="fk_patient__etablissement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="etablissement"
                                 />
    </changeSet>
</databaseChangeLog>
